{
    "functions": {
        "api/my-api.ts": {
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/api/rss",
            "dest": "/api/rss.ts"
        }
    ],
    "rewrites": [
        {
            "source": "/api",
            "destination": "/api/rss.js"
        },
        {
            "source": "/api/rss",
            "destination": "/api/rss.cjs"
        },
        {
            "source": "/roster",
            "destination": "/index.html"
        },
        {
            "source": "/showcase",
            "destination": "/index.html"
        },
        {
            "source": "/policies",
            "destination": "/index.html"
        },
        {
            "source": "/exclusive",
            "destination": "/index.html"
        },
        {
            "source": "/free",
            "destination": "/index.html"
        },
        {
            "source": "/",
            "destination": "/index.html"
        }
    ]
     
}